{
  "version": "1.0.0",
  "description": "ServiceFabric Hosting library (aspnet core).",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/cperras/ServiceFabricHosting"
    },
    "tags": [
      "aspnetcore",
      "servicefabric"
    ]
  },
  "dependencies": {
    "System.Buffers": "4.0.0",
    "System.Runtime.InteropServices.RuntimeInformation": "4.0.0",
    "System.Threading.Tasks.Extensions": "4.0.0",
    "Microsoft.AspNetCore.Hosting": "1.0.0",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.0",
    "Microsoft.ServiceFabric": "5.1.163",
    "Microsoft.ServiceFabric.Data": "2.1.163",
    "Microsoft.ServiceFabric.Services": "2.1.163"

  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        },
        "System.Threading.Tasks": {
          "type": "build"
        }
      },
      "dependencies": {
      }
    }
  },
  "buildOptions": {
    "allowUnsafe": true,
    // "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  }
}

